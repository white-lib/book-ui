{
  "name": "@white-lib/book-ui",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": {
    "name": "Hrach Martirosyan",
    "url": "https://www.linkedin.com/in/hrachmartirosyan/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "build:watch": "tsc --p ./tsconfig-build.json && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "scss:watch": "sass --watch lib:lib",
    "scss:build": "sass lib:lib",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "9.16.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^9.1.1",
    "glob": "^11.0.1",
    "prettier": "^3.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-preserve-directives": "^1.1.3",
    "sass": "^1.84.0",
    "storybook": "^9.1.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/white-lib/book-ui.git"
  },
  "keywords": [
    "ui",
    "library",
    "kit",
    "design",
    "react",
    "react.js",
    "lightweight",
    "fast",
    "performant",
    "lite",
    "small"
  ],
